#include <stdio.h>

int main() {
    int n, sum = 0;
    
   
    printf("Enter the number of integers: ");
    scanf("%d", &n);
    
    
    int numbers[n];

    
    printf("Enter %d integers:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &numbers[i]);
        sum += numbers[i]; 
    }
    
    
    int average = sum / n;

   
    printf("The average is: %d\n", average);

    return 0;
}
